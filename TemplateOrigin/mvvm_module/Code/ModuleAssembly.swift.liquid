import UIKit
import Swinject

struct {{ module_info.name }}ModuleAssembly: Assembly {

  func assemble(container: Container) {
    container.register({{ module_info.name }}Module.self) { resolver in
      let controller = {{ module_info.name }}ViewController()
      let viewModel = {{ module_info.name }}ViewModel()

      controller.viewModel = viewModel
      return controller
    }
  }
}
